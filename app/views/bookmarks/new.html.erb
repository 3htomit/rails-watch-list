<hr>
<h2> Create a new bookmark </h2>
<br>
<%= simple_form_for(@bookmark) do |f| %>
  <%= f.association :list %>
  <%= f.association :movie %>
  <%= f.input :comment %>
  <%= f.submit "Add bookmark", class: "btn btn-primary mt-2" %>
  <%= link_to "Cancel", :back, class: "btn btn-outline-warning mt-2"%>
<% end %>
